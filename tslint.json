{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-empty": false,
    "no-empty-interface": false,
    "no-console": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "object-literal-shorthand": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
